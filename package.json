{
  "name": "labelgen-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run generate:sqlite & next dev",
    "dev:postgres": "npm run generate:postgres & next dev",
    "build": "next build && npx next-swagger-doc-cli src/lib/next-swagger-doc.json",
    "vercel-build": "npm run generate:postgres && npm run migrate-deploy:postgres && next build",
    "migrate-deploy:postgres": "prisma migrate deploy --schema=./prisma_postgres/schema.prisma",
    "migrate-deploy:sqlite": "prisma migrate deploy --schema=./prisma_sqlite/schema.prisma",
    "generate:postgres": "prisma generate --schema=./prisma_postgres/schema.prisma",
    "generate:sqlite": "prisma generate --schema=./prisma_sqlite/schema.prisma",
    "create-migration:postgres": "node create-postgres-migration.js",
    "create-migration:sqlite": "node create-sqlite-migration.js",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "format": "prettier --check --ignore-path .prettierignore .",
    "format:fix": "prettier --write --ignore-path .prettierignore ."
  },
  "dependencies": {
    "@clerk/nextjs": "^4.29.12",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.9.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@types/lodash": "^4.14.202",
    "@vercel/postgres": "^0.8.0",
    "ag-grid-community": "^31.0.3",
    "ag-grid-react": "^31.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "generate-api-key": "^1.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.330.0",
    "next": "14.1.0",
    "next-swagger-doc": "^0.4.0",
    "papaparse": "^5.4.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.50.1",
    "swagger-ui-react": "^5.3.2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/swagger-ui-react": "^4.18.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma": "^5.9.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "uuid": "^9.0.1"
  }
}
