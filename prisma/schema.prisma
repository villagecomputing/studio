generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Column {
  id           Int          @id @default(autoincrement())
  dataset_id   Int
  name         String
  field        String
  index        Int
  type         String
  created_at   DateTime     @default(now())
  updated_at   DateTime     @updatedAt
  deleted_at   DateTime?
  Dataset_list Dataset_list @relation(fields: [dataset_id], references: [id], onDelete: Cascade)
}

model Dataset_list {
  id         Int       @id @default(autoincrement())
  name       String
  created_at DateTime  @default(now())
  deleted_at DateTime?
  updated_at DateTime  @updatedAt
  Column     Column[]
}
